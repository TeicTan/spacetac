{
    "compilerOptions": {
        "module": "system",
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "out": "out/build.js",
        "strict": true,
        "sourceMap": true,
        "target": "es5"
    },
    "include": [
        "src"
    ]
}